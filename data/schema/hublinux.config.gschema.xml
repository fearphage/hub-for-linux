<schemalist>
    <schema id="hublinux.config" path="/apps/hublinux/">
        <key type="s" name="github-login">
            <default>""</default>
            <summary>The Github login</summary>
            <description>Login data (username/email) for github.</description>
        </key>
        <key type="s" name="git-path">
            <default>"~/"</default>
            <summary>Local git-path.</summary>
            <description>Path to local git directories.</description>
        </key>
    </schema>
</schemalist>